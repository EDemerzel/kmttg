<?xml version="1.0" encoding="UTF-8"?>
<?scenebuilder-stylesheet ../../../../../../release/css/kmttg.css?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tivo.kmttg.gui.remote.channels"
    fx:id="panel" spacing="1">
    <HBox spacing="5" alignment="CENTER_LEFT">
    <padding><Insets top="5" right="0" bottom="0" left="5"/></padding>
        <Label text="Channels"/>
        <Label /><!-- tivo_label... has no content. -->
        <ChoiceBox fx:id="tivo"></ChoiceBox>
        <Button fx:id="refresh" text="Refresh" onAction="#refreshCB"/>
        <Button fx:id="save" text="Save..." onAction="#saveCB"/>
        <Button fx:id="load" text="Load..." onAction="#loadCB"/>
        <Button fx:id="copy" text="Copy" onAction="#copyCB"/>
        <Button fx:id="update" text="Modify" onAction="#updateCB"/>
        <Label fx:id="label" />
        
    </HBox>
</VBox>

